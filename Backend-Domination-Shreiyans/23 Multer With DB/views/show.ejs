<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Uploaded Files</title>
  </head>
  <body>
    <% files.forEach((file) => { %>
    <div>
      <p><%= file.name %></p>
      <img
        src="data:image/jpg;base64, <%= file.image.toString('base64') %>"
        alt="<%= file.name %>"
      />
    </div>
    <% }) %>
  </body>
</html>
